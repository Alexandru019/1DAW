import java.util.Arrays;

public class ConjuntoDeFiguras {
 private Figura[]A;
 static private int posicion=0;
 ConjuntoDeFiguras(){
 A = new Figura[10];
 }
 
 void a√±ade(Figura f){ 
	 A[posicion]=f;
	 posicion++;
 }

 double areaTotal(){
	 return 2;
 }

public void imprimeTodo() {
	for(int i=0;i<posicion;i++) {
		System.out.println("Color: "+A[i].DimeColor());
	}
}
 
}
